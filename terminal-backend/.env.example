# Server Configuration
PORT=6000
NODE_ENV=development

# JWT Configuration
JWT_SECRET=your-secret-key-here-change-in-production
JWT_EXPIRE_TIME=24h

# Logging
LOG_LEVEL=debug

# Terminal Configuration
MAX_TERMINAL_SESSIONS=10
TERMINAL_TIMEOUT=600000

# Security
ALLOWED_ORIGINS=http://localhost:5173,http://localhost:3000

# Aliyun SenseVoice API Configuration
ALIYUN_API_KEY=your_sensevoice_api_key_here

# Aliyun OSS Configuration
OSS_ACCESS_KEY_ID=your_oss_access_key_id
OSS_ACCESS_KEY_SECRET=your_oss_access_key_secret
OSS_BUCKET=your_oss_bucket_name
OSS_REGION=oss-cn-hangzhou

# OSS Configuration for Transcription Service (Optional)
# If not set, will use the default OSS configuration above
OSS_TRANSCRIPTION_ACCESS_KEY_ID=your_transcription_oss_access_key_id
OSS_TRANSCRIPTION_ACCESS_KEY_SECRET=your_transcription_oss_access_key_secret
OSS_TRANSCRIPTION_BUCKET=your_transcription_bucket_name
OSS_TRANSCRIPTION_REGION=oss-cn-hangzhou